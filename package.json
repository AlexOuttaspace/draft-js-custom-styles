    "babel-preset-react": "^6.1.18",
    "body-parser": "1.15.2",
    "css-loader": "0.25.0",
    "enzyme": "2.5.1",
    "eslint": "3.7.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-flowtype": "2.32.1",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "express": "4.14.0",
