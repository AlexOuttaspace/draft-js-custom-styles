    "lint": "./node_modules/eslint/bin/eslint.js . --ext .js --ext .jsx || true",
    "flow": "./node_modules/flow-bin/vendor/flow; test $? -eq 0 -o $? -eq 2"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "7.0.0",
