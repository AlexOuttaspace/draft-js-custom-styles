    "draft-js-export-html": "0.5.2",
    "draft-js-plugins-editor": "2.0.0-beta1",
    "immutable": "3.8.1",
    "jsdom": "^8.1.0",
    "lodash.camelcase": "4.3.0",
    "lodash.snakecase": "4.1.1",
    "react": "latest",
    "react-addons-test-utils": "latest",
